source {
	repo = "https://github.com/jumidev/terraform-modules-aws.git"
	path = "network/subnet"
}

inputs {
	name = "example subnet ${run_id}"
	cidr_block = "192.168.1.0/24"
}

tfstate_store {
	bucket = "${test_bucket}"
	bucket_path = "test/${run_id}/${COMPONENT_PATH}"
}

tfstate_inputs {
	vpc_id = "aws/vpc_tfstate"
}